{
  "name": "fajitas",
  "version": "1.0.0",
  "description": "A WebGL fractal renderer",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "test-watch": "mocha --watch --require babel-core/register",
    "watch": "webpack --watch --progress",
    "build": "webpack --progress",
    "build-parser": "pegjs -o src/parser.js src/parser.pegjs",
    "build-prod": "webpack --progress -p",
    "elm": "elm",
    "elm-format": "elm-format --yes elm-src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/titouanc/fajitas.git"
  },
  "author": "iTitou",
  "license": "Beerware",
  "bugs": {
    "url": "https://github.com/titouanc/fajitas/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "jquery": "<3",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.7"
  }
}
